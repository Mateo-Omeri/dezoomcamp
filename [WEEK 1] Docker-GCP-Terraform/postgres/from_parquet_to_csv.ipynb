    "# Read parquet file\n",
    "parquet_file = pq.read_table('yellow_tripdata_2021-01.parquet')\n",
    "\n",
    "# Transform into datafrme\n",
    "df = parquet_file.to_pandas()\n",
    "\n",
    "# Save it as csv file\n",
    "df.to_csv('yellow_tripdata_2021-01.csv', index=False)\n"
   ]
